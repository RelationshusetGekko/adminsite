- content_for :header_left do
  Edit
  = @page.title
- content_for :header_right do
  = link_to_back 'Back', admin_pages_path

= error_messages_for @page

= form_for([:admin, @page]) do |f|
  #page-edit
    = render :partial => 'form', :locals => {:f => f}
    = submit_tag "Update"
= render :partial => 'admin/shared/syntax_help'
#page-assets-list
  %h2 Assets
  %table
    - @file_assets.each do |asset|
      %tr
        %td= asset.attachment_file_name
.clear
